{
  "name": "pino-logfmt",
  "version": "1.1.0",
  "description": "Logfmt transport for pino",
  "main": "./src/transport.js",
  "bin": "./src/bin.js",
  "typings": "./typings/types.d.ts",
  "scripts": {
    "test": "TZ=Europe/Paris node --test tests/*.spec.js",
    "lint": "standard --fix \"src/**/*.js\" \"tests/**/*.js\"",
    "v": "npx @changesets/cli version && npx --no-install synclockver"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botflux/pino-logfmt.git"
  },
  "keywords": [
    "pino",
    "logfmt"
  ],
  "author": "Victor Mendele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/botflux/pino-logfmt/issues"
  },
  "type": "module",
  "homepage": "https://github.com/botflux/pino-logfmt#readme",
  "devDependencies": {
    "@changesets/cli": "2.29.8",
    "@types/logfmt": "1.2.6",
    "@types/pump": "1.1.3",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "pino": "10.3.0",
    "standard": "17.1.2",
    "synclockver": "1.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.9"
  },
  "dependencies": {
    "case-anything": "3.1.2",
    "dateformat": "5.0.3",
    "logfmt": "1.4.0",
    "pino-abstract-transport": "3.0.0",
    "pump": "3.0.3",
    "sonic-boom": "4.2.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "standard --fix"
    ],
    "tests/**/*.js": [
      "standard --fix"
    ]
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
